import math

#Criar um algoritmo que entre com o valor dex calcule e imprima o valor de f(x)

def f(x):
    return (5 * x + 3) / math.sqrt(x**2 - 16)

x = float(input("Digite o valor de x: "))
 
if x > 4 and x < -4:
    fx = f(x)
    print(f"\nf(x) = {fx}")
else:
    print("NAO PODE SER FEITA")
